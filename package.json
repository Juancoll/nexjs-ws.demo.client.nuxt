{
  "name": "nuxt-auth",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:debug": "node --inspect=localhost:9230 node_modules/@nuxt/typescript-runtime/bin/nuxt-ts.js",
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "ws:update": "  nexjs ts ws   up-client -s ../nexjs-ws.demo.server.nestjs                        -o ./lib/clients/ws",
    "http:update": "nexjs ts http up-client -s ../nexjs-ws.demo.server.nestjs/dist/swagger-spec.json -o ./lib/clients/http"
  },
  "dependencies": {
    "@nexjs/wsclient": "^1.0.6",
    "@nuxt/content": "^1.5.0",
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/auth-next": "^5.0.0-1595976864.509d9d6",
    "@nuxtjs/axios": "^5.12.2",
    "@types/node": "^14.6.3",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cookieparser": "^0.1.0",
    "express-session": "^1.17.1",
    "js-cookie": "^2.2.1",
    "morgan": "^1.10.0",
    "nuxt": "^2.14.0",
    "nuxt-property-decorator": "^2.7.2",
    "nuxt-vuex-localstorage": "^1.2.7",
    "vue-class-component": "^7.2.5"
  },
  "devDependencies": {
    "@nexjs/cli": "^1.5.0",
    "@nuxt/types": "^2.14.0",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/express-session": "^1.17.0",
    "@types/js-cookie": "^2.2.6",
    "@types/morgan": "^1.9.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0"
  }
}
